{
  "name": "my-nuxt-app",
  "devDependencies": {
    "concurrently": "^9.1.2"
  },
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=nuxt-app\" \"npm run dev --workspace=studio\"",
    "build": "npm run build --workspace=nuxt-app && npm run build --workspace=studio",
    "start": "npm run start -w nuxt-app"
  },
  "workspaces": [
    "nuxt-app",
    "studio"
  ]
}
